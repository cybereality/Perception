			<ExcludeGroup>IfZero</ExcludeGroup>
			<Comparisons>
				<Compare type="NotEqual">
					<MinValue>00000000</MinValue>
				</Compare>
			</Comparisons>
		</ScanRule>
			
		<ScanRule>
			<AxisName>TrackerRoll</AxisName>
			<AddressType>DWORD</AddressType>
