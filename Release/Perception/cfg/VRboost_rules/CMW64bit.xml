			<ExcludeGroup>NoExclude</ExcludeGroup>
			<Comparisons>
				<Compare type="Equal">
					<MinValue>120.0</MinValue>
				</Compare>
			</Comparisons>
		</ScanRule>
	</ScannerGroup>
  </ScannerGroups>
