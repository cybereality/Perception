			<Comparisons>
				<Compare type="Equal">
					<MinValue>00000000</MinValue>
				</Compare>
			</Comparisons>
		</ScanRule>
			
		<ScanRule>
			<AxisName>WorldFOV</AxisName>
			<AddressType>Float</AddressType>
